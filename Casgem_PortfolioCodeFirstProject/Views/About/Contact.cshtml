@model List<Casgem_PortfolioCodeFirstProject.DAL.Entities.Category>
<section class="w3l-footer-29-main">
    <div class="footer-29 py-5 ">
        <div class="container">
            <div class="grid-col-4 footer-top-29">
                <div class="footer-list-29 footer-1">
                    <h2 class="footer-title-29">
                        Bize Ulaşın
                    </h2>
                    <ul>
                        <li>
                            <p>
                                <span class="fa fa-map-marker"></span>@ViewBag.Adress
                            </p>
                        </li>
                        <li><a href="tel:@ViewBag.Telefon"><span class="fa fa-phone"></span> @ViewBag.Telefon</a></li>
                        <li><a href="mailto:@ViewBag.Mail" class="mail"><span class="fa fa-envelope-open-o"></span> @ViewBag.Mail</a></li>
                    </ul>
                    <div class="main-social-footer-29">
                        <a href="@ViewBag.FaceURl" class="facebook"><span class="fa fa-facebook"></span></a>
                        <a href="@ViewBag.TwitterURL" class="twitter"><span class="fa fa-twitter"></span></a>
                        <a href="@ViewBag.InstagramURl" class="instagram"><span class="fa fa-instagram"></span></a>
                        <a href="@ViewBag.MailURl" class="google-plus"><span class="fa fa-google-plus"></span></a>
                        <a href="@ViewBag.LinkedinURl" class="linkedin"><span class="fa fa-linkedin"></span></a>
                    </div>
                </div>
                <div class="footer-list-29 footer-2">
                    <ul>

                        <h6 class="footer-title-29">Kategori</h6>

                        @foreach (var item in Model)
                        {
                            <li><a href="#pages">@item.CategoryURL</a></li>
                        }


                    </ul>
                </div>



                @Html.Action("Gallery")

                @Html.Action("Travel")


            </div>

        </div>
    </div>
</section>